  <button class="fab-toggle-nihgtly" id="toggle-nightify-btn">Toggle</button>

    <script src="/js/nightly.js" charset="utf-8"></script>
    <script type="text/javascript">
      var sayGoodNight = function () {
        console.log('Good night :) !');
      };

      var sayGoodMorning = function () {
        console.log('Good morning !');
      };

      // Persistence disabled
      var Nightly = new Nightly(
        {
          classes: [
            {
              apply: 'hilighted-text-dark',
              to: '.hilighted-text',
            },
            {
              apply: 'comment-section-dark',
              to: '.comment-section',
            },
            {
              apply: 'comment-card-dark',
              to: '.comment-card',
            },
          ],
        },
        false
      );

      document
        .getElementById('toggle-nightify-btn')
        .addEventListener('click', function () {
          // toggle() function takes two optional callbacks
          Nightly.toggle(sayGoodNight, sayGoodMorning);
        });
    </script>



